#
# usage: simpleLinRegression [-x xColumn] [-y yColumn] [file(s)] [<file]
#

use strict;
use warnings;

use lib('/usr/lib/R/library/RSPerl/scripts',
	'/usr/lib/R/library/RSPerl/share/lib/perl5/site_perl/5.6.1/i386-linux/'
	);

$ENV{R_HOME}='/usr/lib/R' unless $ENV{R_HOME};

use R;
use RReferences;

# use Statistics::OLS; 

use Getopt::Std;
getopt('xy') ;

our ($opt_x,$opt_y);

$opt_x=1 unless $opt_x;
$opt_y=2 unless $opt_y;

--$opt_x; --$opt_y;

#
# either run for each file specified or run on stdin
#

my $notdone=1;
my $file='';

while ($notdone) {
    if (@ARGV) {
	$file=shift @ARGV;
	close(STDIN);
	open(STDIN,"<$file") or die "Failed <$file\n";
    }
    my @xdata=();
    my @ydata=();
    while (<STDIN>) {
	next unless /^\s*[0-9.+-]/;  # doesn't start with a number -> skip
	chomp;
	my @f=split('\t');
	push @xdata,0+$f[$opt_x];  # Perl bug? need to convert to number!
	push @ydata,0+$f[$opt_y];
    }
    close(STDIN);
    R::initR('--vanilla','--gui=none', '--slave');
    R::library('RSPerl');
    my $fR=R::call('formula','y~x');
    my $dfR=R::callWithNames('data.frame',{x=>\@xdata,y=>\@ydata});
    my $regrR=R::call('lm',$fR,$dfR);
    my $sR=R::call('summary',$regrR);
    my $coefR=R::call('coef',$sR);
    my @coefVals=R::call('as.vector',$coefR);
    my @cnames=qw(intercept slope stderr_intercept stderr_slope
		  t_intercept t_slope p_intercept p_slope);
    my %coef=map { $cnames[$_]=>$coefVals[$_] } 0..$#cnames;
    print join("\t",$file,(map {$_.'='.$coef{$_}} @cnames)),"\n";
    $notdone=@ARGV;
    print "notdone=$notdone\n";
}
close(STDIN);


