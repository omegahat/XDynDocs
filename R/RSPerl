#
# usage: simpleLinRegression [-x xColumn] [-y yColumn] [file(s)] [<file]
#

use strict;
use warnings;

use lib('/usr/lib/R/library/RSPerl/scripts',
	'/usr/lib/R/library/RSPerl/share/lib/perl5/site_perl/5.6.1/i386-linux/'
	);

$ENV{R_HOME}='/usr/lib/R' unless $ENV{R_HOME};

use R;
use RReferences;

# use Statistics::OLS; 

use Getopt::Std;
getopt('xy') ;

our ($opt_x,$opt_y);

$opt_x=1 unless $opt_x;
$opt_y=2 unless $opt_y;

--$opt_x; --$opt_y;

#
# either run for each file specified or run on stdin
#

my $notdone=1;
my $file='';

while ($notdone) {
    if (@ARGV) {
	$file=shift @ARGV;
	close(STDIN);
	open(STDIN,"<$file") or die "Failed <$file\n";
    }
    my @xdata=();
    my @ydata=();
    while (<STDIN>) {
	next unless /^\s*[0-9.+-]/;  # doesn't start with a number -> skip
	chomp;
