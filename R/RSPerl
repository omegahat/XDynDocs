use strict;
use IO::File;
use Data::Dumper;
use R;
use RReferences;

my $input = IO::File->new("<$ARGV[0]") or die "Unable to open input 
file: $!";
my $out = IO::File->new(">$ARGV[1]") or die "Unable to open output file: 
$!";
my @data = ();
my @row = ();

&R::initR("--slave");                 #start R program and load libraries
&R::library("RSPerl");
&R::library("compositeLD");

while(<$input>)
{
    chomp;
    if($. == 1){ next; }
    @row = split /,/;

  #  print Dumper(@row);
}

my @locusA = $row[1];
my @locusA2 = $row[2];
my @locusB = $row[3];
my @locusB2 = $row[4];


my @x =  &R::call("compositeLD",\@locusA,\@locusA2,\@locusB,\@locusB2);

print "@x\n";

