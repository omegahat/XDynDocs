#############eval.pl##########################3

#! /usr/bin/env perl
# the above line use the perl interpreter as found in the PATH
# It is preferred as the usual way with an absolute path for 
multi-platform use
# and portability. Moreover, to execute your scripts with another perl 
interpreter
# version, you only have to change your PATH, not the scripts.
{
use warnings;    # we want the warnings, because we are careful coders ;-)
use strict;
use FileHandle;
use Carp qw(carp cluck confess croak);

use IO::Zlib;



my $fh = \*STDIN;
my $file = $ARGV[0];

if ( !defined $file && -t STDIN ) {
  warn "No input data\n";
  exit(0);
}

if ($file) {
    $fh = &openFile($file);
}
}

#had to be global ?
$data = getDumpData($fh);


exit(0);


######## Subs #############################33

=head2 openFile

open a file. can open a gzipped file. 

autodetect a gzipped file from the suffix (.gz)
B<Arg> : string - the filename

B<Return> : a FileHandle, or undef on error

B<Exception> : warn on error

=cut

sub openFile {
    my ($filename) = @_;

    unless ( $filename ) {
        cluck "Bad Arg, you must give a filename";
        return undef;
    }

    unless ( -e $filename ) {
        cluck "file [$filename] does not exist, aborting...";
        return undef;
    }
 

    my $fh;
    if ( $filename =~ /\.gz$/ ) { # gzipped file
 
        if ( $filename =~ /\.gbd\.gz/ ) { # gbd gzipped file
            $fh = FileHandle->new( "gunzip -c $filename |" );
        } else {                # IO::Zlib handles are not compatible with 
Storable/GBD format
            #require IO::Zlib;
            $fh = IO::Zlib->new($filename,"rb");
        }
    } else {
        $fh = FileHandle->new( $filename );
    }

    unless ( $fh ) {
        cluck "unable to open file [$filename] : $!";
    }

    return $fh;
}

sub getDumpData {

    my ($fh) = @_;
    return undef unless defined $fh;
    my $line = join("",<$fh>);
    close $fh;
    my $VAR1;
    $VAR1 = eval $line;
    if ($@) {
        die "error while undumping file, SPA dump seems corrupted : $@";
    }
    return $VAR1;
}
