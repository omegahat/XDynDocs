PAZAR_user=pazar_rw
PAZAR_pass=pazar_rwpass
QTDIR=/usr/lib/qt-3.1
SSH_TTY=/dev/pts/0
PAZAR_drv=mysql
USER=root
LD_LIBRARY_PATH=/space/Progeny/ProgenyServer/lib:/space/Progeny/ProgenyServer/jre142/lib/i386/client:/space/Progeny/ProgenyServer/jre142/lib/i386:/space/Progeny/ProgenyServer/drivers/lib:

