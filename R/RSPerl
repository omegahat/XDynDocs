diff -ur RSPerl_0.6-1/src/Reflectance.c RSPerl_0.6-1.hacked/src/Reflectance=
.c
--- RSPerl_0.6-1/src/Reflectance.c	2004-09-26 12:59:53.000000000 -0400
+++ RSPerl_0.6-1.hacked/src/Reflectance.c	2004-09-28 19:54:57.000000000 -04=
00
@@ -473,7 +473,11 @@
=20
       if(!el)
 	  continue;
-      if(type !=3D SVt_NULL && type !=3D SvTYPE(el)) {
+      if(type !=3D SVt_NULL && type !=3D SvTYPE(el) &&=20
+	 !((type =3D=3D SVt_NV && SvTYPE(el) =3D=3D SVt_PVNV) ||
+	   (type =3D=3D SVt_PVNV && SvTYPE(el) =3D=3D SVt_NV) ||
+	   (type =3D=3D SVt_IV && SvTYPE(el) =3D=3D SVt_PVIV) ||
+	   (type =3D=3D SVt_PVIV && SvTYPE(el) =3D=3D SVt_IV))) {
   	  ok =3D FALSE;
           return(FALSE);
       }

