&nbsp; 'swi_name' =&gt; undef,</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
&nbsp; 'gene_name' =&gt; 'BrightCorner',</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
&nbsp; 'seq' =&gt; 'ATCATCGTAGCTGGTCAGTGTATCCTTTTTTTTTTATCATCGTAGCTGGTCAGTGTATCC'</font>
