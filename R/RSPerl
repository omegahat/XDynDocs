<br>
<br>
<br><font size=2 face="sans-serif">#############eval.pl##########################3</font>
<br><font size=2 face="sans-serif"><br>
#! /usr/bin/env perl</font>
<br><font size=2 face="sans-serif"># the above line use the perl interpreter
as found in the PATH</font>
<br><font size=2 face="sans-serif"># It is preferred as the usual way with
an absolute path for multi-platform use</font>
<br><font size=2 face="sans-serif"># and portability. Moreover, to execute
your scripts with another perl interpreter</font>
<br><font size=2 face="sans-serif"># version, you only have to change your
PATH, not the scripts.</font>
<br><font size=2 face="sans-serif">{</font>
<br><font size=2 face="sans-serif">use warnings; &nbsp; &nbsp;# we want
the warnings, because we are careful coders ;-)</font>
<br><font size=2 face="sans-serif">use strict;</font>
<br><font size=2 face="sans-serif">use FileHandle;</font>
<br><font size=2 face="sans-serif">use Carp qw(carp cluck confess croak);</font>
<br>
<br><font size=2 face="sans-serif">use IO::Zlib;</font>
<br>
<br>
<br>
<br><font size=2 face="sans-serif">my $fh = \*STDIN;</font>
<br><font size=2 face="sans-serif">my $file = $ARGV[0];</font>
<br>
<br><font size=2 face="sans-serif">if ( !defined $file &amp;&amp; -t STDIN
) {</font>
<br><font size=2 face="sans-serif">&nbsp; warn &quot;No input data\n&quot;;</font>
<br><font size=2 face="sans-serif">&nbsp; exit(0);</font>
<br><font size=2 face="sans-serif">}</font>
<br>
<br><font size=2 face="sans-serif">if ($file) {</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; $fh = &amp;openFile($file);</font>
<br><font size=2 face="sans-serif">}</font>
<br><font size=2 face="sans-serif">}</font>
<br>
<br><font size=2 face="sans-serif">#had to be global ?</font>
<br><font size=2 face="sans-serif">$data = getDumpData($fh);</font>
<br>
<br>
<br><font size=2 face="sans-serif">exit(0);</font>
<br>
<br>
<br><font size=2 face="sans-serif">######## Subs #############################33</font>
<br>
<br><font size=2 face="sans-serif">=head2 openFile</font>
<br>
<br><font size=2 face="sans-serif">open a file. can open a gzipped file.
</font>
<br>
<br><font size=2 face="sans-serif">autodetect a gzipped file from the suffix
(.gz)</font>
<br><font size=2 face="sans-serif">B&lt;Arg&gt; : string - the filename</font>
<br>
<br><font size=2 face="sans-serif">B&lt;Return&gt; : a FileHandle, or undef
on error</font>
<br>
<br><font size=2 face="sans-serif">B&lt;Exception&gt; : warn on error</font>
<br>
<br><font size=2 face="sans-serif">=cut</font>
<br>
<br><font size=2 face="sans-serif">sub openFile {</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; my ($filename) = @_;</font>
<br>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; unless ( $filename ) {</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; cluck &quot;Bad
Arg, you must give a filename&quot;;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; return undef;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; }</font>
<br>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; unless ( -e $filename
) {</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; cluck &quot;file
[$filename] does not exist, aborting...&quot;;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; return undef;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; }</font>
<br><font size=2 face="sans-serif">&nbsp; </font>
<br>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; my $fh;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; if ( $filename =~ /\.gz$/
) { # gzipped file</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; </font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; if ( $filename
=~ /\.gbd\.gz/ ) { # gbd gzipped file</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
$fh = FileHandle-&gt;new( &quot;gunzip -c $filename |&quot; );</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; } else {
&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;# IO::Zlib handles
are not compatible with Storable/GBD format</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
#require IO::Zlib;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;
$fh = IO::Zlib-&gt;new($filename,&quot;rb&quot;);</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; }</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; } else {</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; $fh = FileHandle-&gt;new(
$filename );</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; }</font>
<br>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; unless ( $fh ) {</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; cluck &quot;unable
to open file [$filename] : $!&quot;;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; }</font>
<br>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; return $fh;</font>
<br><font size=2 face="sans-serif">}</font>
<br>
<br><font size=2 face="sans-serif">sub getDumpData {</font>
<br>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; my ($fh) = @_;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; return undef unless defined
$fh;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; my $line = join(&quot;&quot;,&lt;$fh&gt;);</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; close $fh;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; my $VAR1;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; $VAR1 = eval $line;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; if ($@) {</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; &nbsp; &nbsp; die &quot;error
while undumping file, SPA dump seems corrupted : $@&quot;;</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; }</font>
<br><font size=2 face="sans-serif">&nbsp; &nbsp; return $VAR1;</font>
<br><font size=2 face="sans-serif">}<br>
----------------------------------------------------------------------<br>
Marc Lamarine<br>
<br>
Serono Pharmaceutical Research Institute<br>
14, chemin des Aulx - 1228 Plan-les-Ouates<br>
Geneva, Switzerland<br>
